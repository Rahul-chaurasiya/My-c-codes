


#include<stdio.h>
#include<conio.h>
 main()
{
   int n,r,add=0,rev=0;


   printf("Please enter any  number ");
   scanf("%d",&n);

     while(n>0)
     {
       r=n%10;
       n=n/10;
       rev=rev*10+r;
     }

     while(rev>0)
     {
       r=rev%10;
       rev=rev/10;
       r=r+1;
       r=r%10;
       add=add*10+r;
     }

    printf("%d",add);


}

output :
Please enter any number 345678
456789
