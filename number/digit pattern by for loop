

#include<stdio.h>
#include<conio.h>
main()
{
    int i,j;
    
    for(i=1;i<=3;i++)
    {
      for(j=1;j<=5;j++)
        { 
          if (j<=i+2 && j>=4-i)
           printf("%d",i);
          else
           printf(" ");
        }
          printf("\n");
    }
    
    getch();
    
}

output :
  1  
 222 
33333
