


#include<stdio.h>
#include<conio.h>
 main()
{
   int i=1,count=0,n;
   
   printf("enter a number ");
   scanf("%d",&n);

   for(i=1;i<=n;i++)
   {
    if(n%i==0)
    {
      count++;
    }
   }

   if (count==2)
   {
     printf("\n the no. is a prime no. ");
   }

   else
   printf("\n the no. is not a prime no.");


}

output 1 :
enter a number 17
the no. is a prime no.

output 2 :
enter a number 21
the no. is not a prime no.
