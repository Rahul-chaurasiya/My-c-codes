

#include<stdio.h>
#include<conio.h>
main()
{
            int a[100],i,n,b,c,sum;

  printf("enter the size of an array :");
  scanf("%d",&n);

printf("enter the elements of array :\n ");

      for (i=0;i<n;i++)
      {
            scanf("%d",&a[i]);
      }
      
 printf("\nyr array is :");

     for (i=0;i<n;i++)
     {
         printf("\n%d",a[i]);
     }

printf("\nreverse of yr array is : ");

      for (i=n-1;i>=0;i--)
     {
         printf("\n %d",a[i]);
     }
     
getch();

}

