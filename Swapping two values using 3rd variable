void main()
{
          int a,b,temp;
          clrscr();

    printf("enter the value of a and b\n");
    scanf("%d%d",&a,&b);

    printf("before swiping value of a=%d & b=%d\n",a,b);

          temp=a;
          a=b;
          b=temp;

    printf("after swiping value of a=%d & b=%d",a,b);
    
    getch();

}

