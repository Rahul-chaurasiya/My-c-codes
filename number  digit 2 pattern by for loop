

#include<stdio.h>
#include<conio.h>
main()
{
    int i,j;
    
    for(i=1;i<=5;i++)
    { if(i%2!=0){
      for(j=1;j<=5;j++)
        { 
          if (j>=6-i)
           printf("%d",i);
          else
           printf(" ");
        }}
        
          printf("\n");
    }
    
    getch();

}

output :
    1
     
  333
     
55555
