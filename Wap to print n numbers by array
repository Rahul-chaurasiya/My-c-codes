


#include<stdio.h>
#include<conio.h>
 main ()
{

        int a[100],i,n;

  printf("enter the size of an array :");
  scanf("%d",&n);

printf("enter the elements of array :\n ");
    for (i=0;i<n;i++)
  { 
      scanf("%d",&a[i]);
  }

 printf("\nyour array is :");
    for (i=0;i<n;i++)
   {
      printf("\n%d",a[i]);
   }

getch ();

}

