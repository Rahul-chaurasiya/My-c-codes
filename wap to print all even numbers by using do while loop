


#include<stdio.h>
#include<conio.h>
 main ()
{
     int n,i=0;

printf("print all even no. untill \n");
scanf("%d",&n);
printf("even no. from 1 to %d \n ",n );
do{
if(i%2 == 0)
  {
    printf(" \n %d ",i);
  }
  i++;
}
while(i<=n);


}

output :
print all even no. untill
7
